{
  "id": "com.rohitchouhan.dynamoTable",
  "name": "Dynamo Table",
  "version": "1.0.0",
  "description": "A custom widget to help you create custom tables.",
  "newInstancePrefix": "dynamoTable",
  "vendor": "rohitchouhan.com",
  "eula": "rohitchouhan.com",
  "license": "MIT",
  "icon": "https://cdn.jsdelivr.net/gh/SAP-Custom-Widget/dynamoTable@1.0.0/icon.png",
  "imports": [
    "datasource",
    "planning-model"
  ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "dynamo-table",
      "url": "https://cdn.jsdelivr.net/gh/SAP-Custom-Widget/dynamoTable@1.0.0/dynamoTable.bundle.js",
      "integrity": "sha256-rpQ7evmwjSTNGlnvP6GNdnU3in0Ut5q2NlxgA4sFcnA=",
      "ignoreIntegrity": false
    },
    {
      "kind": "builder",
      "tag": "dynamo-table-builder",
      "url": "https://cdn.jsdelivr.net/gh/SAP-Custom-Widget/dynamoTable@1.0.0/DynamoTable.bundle.js",
      "integrity": "sha256-rpQ7evmwjSTNGlnvP6GNdnU3in0Ut5q2NlxgA4sFcnA=",
      "ignoreIntegrity": false
    }
  ],
  "properties": {
    "tableTitle": {
      "description": "Name of Table",
      "type": "string",
      "default": "Dynamo Table"
    },
    "searching": {
      "type": "boolean",
      "default": true
    }
  },
  "methods": {
    "setTableData": {
      "description": "Sets the table data.",
      "parameters": [
        {
          "name": "RecordsArray",
          "type": "Selection[]"
        }
      ]
    }
  },
  "events": {}
}